<h1>Listing user_addresses</h1>

<table>
  <tr>
    <th>Userid</th>
    <th>Is current</th>
    <th>{</th>
    <th>Addr 1</th>
    <th>Addr 2</th>
    <th>City</th>
    <th>State</th>
    <th>Zip</th>
    <th>Addr</th>
    <th>{</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @user_addresses.each do |user_address| %>
  <tr>
    <td><%= user_address.userid %></td>
    <td><%= user_address.is_current %></td>
    <td><%= user_address.{ %></td>
    <td><%= user_address.addr_1 %></td>
    <td><%= user_address.addr_2 %></td>
    <td><%= user_address.city %></td>
    <td><%= user_address.state %></td>
    <td><%= user_address.zip %></td>
    <td><%= user_address.addr_id %></td>
    <td><%= user_address.{ %></td>
    <td><%= link_to 'Show', user_address %></td>
    <td><%= link_to 'Edit', edit_user_address_path(user_address) %></td>
    <td><%= link_to 'Destroy', user_address, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

